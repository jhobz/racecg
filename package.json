{
  "name": "racecg",
  "version": "0.0.0",
  "description": "Graphics builder for restreaming races, tournaments, and other competitive events.",
  "main": "webpack.config.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "tslint 'src/**/*.ts'",
    "watch": "run-p watch:*",
    "watch:dashboard": "parcel watch src/dashboard/*.html --out-dir dashboard --public-url ./",
    "watch:graphics": "parcel watch src/graphics/*.html --out-dir graphics --public-url ./",
    "watch:extension": "tsc -b tsconfig.server.json -w",
    "build": "run-p build:*",
    "build:dashboard": "parcel build src/dashboard/*.html --out-dir dashboard --public-url ./",
    "build:graphics": "parcel build src/graphics/*.html --out-dir graphics --public-url ./",
    "build:extension": "tsc -b tsconfig.server.json"
  },
  "homepage": "github.com/jhobz/racecg",
  "author": {
    "name": "Jeff Hobson",
    "email": "jeffrey296@gmail.com",
    "url": ""
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "speedrun",
    "race",
    "races",
    "restream",
    "restreaming",
    "restreamer",
    "tournament",
    "tournaments",
    "nodecg-bundle"
  ],
  "nodecg": {
    "compatibleRange": "^1.4.1",
    "dashboardPanels": [
      {
        "name": "panel",
        "title": "Panel",
        "width": 5,
        "file": "panel.html",
        "headerColor": "#525F78"
      }
    ],
    "graphics": [
      {
        "file": "index.html",
        "width": 1920,
        "height": 1080
      }
    ]
  },
  "repository": "https://github.com/nodecg/nodecg.git",
  "license": "MIT",
  "devDependencies": {
    "@types/ws": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.18.2",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "twitchps": "github:jhobz/twitchps#bits-v2",
    "ws": "^7.1.1"
  }
}
